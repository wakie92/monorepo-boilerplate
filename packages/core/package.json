{
  "name": "@repo/core",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true
  },
  "scripts": {
    "build": "tsup ",
    "dev": "tsup  --watch"
  },
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    }
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "tsup": "^8.0.2",
    "dayjs": "^1.11.11"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "typescript": "^5.4.5",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0"
  }
}
