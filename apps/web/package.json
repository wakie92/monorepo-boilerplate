{
  "name": "boilerplate-front-end",
  "version": "1.0.0",
  "repository": "https://github.com/b-builders/fe-bolierplate-NextJS",
  "license": "MIT",
  "description": "Boilerplate project to use from when creating new projects.",
  "main": "pages/index.tsx",
  "author": "bbuilders",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "export": "rm -rf .next out && next build && next export",
    "start": "next start",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "next-lint": "next lint",
    "lint": "./node_modules/.bin/eslint --fix ./",
    "cssLint": "stylelint './src/**/styled.ts'",
    "tsLint": "eslint 'src/**/*.[jt]s?(x)'",
    "format": "prettier-eslint --write $PWD/'src/**/*.[jt]s?(x)' && eslint --fix .",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "deploy-serverless": "serverless",
    "pre-commit": "tsc -p tsconfig.json --noEmit && lint-staged",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@next/eslint-plugin-next": "^14.2.3",
    "@tanstack/react-query": "^5.35.1",
    "@tanstack/react-query-devtools": "^5.35.1",
    "@types/lodash-es": "^4.17.12",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.6.8",
    "axios-retry": "^4.1.0",
    "bootstrap": "^5.3.3",
    "dayjs": "^1.11.11",
    "eventemitter3": "^5.0.1",
    "firebase": "^10.11.1",
    "html-react-parser": "^5.1.10",
    "jotai": "^2.8.0",
    "jotai-tanstack-query": "^0.8.5",
    "jwt-decode": "^4.0.0",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "next": "^14.2.3",
    "next-seo": "^6.5.0",
    "nprogress": "^0.2.0",
    "raw-loader": "^4.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-icons": "^5.2.1",
    "react-markdown": "^9.0.1",
    "reactstrap": "^9.2.2",
    "sanitize-html": "^2.13.0",
    "sass": "^1.77.0",
    "styled-components": "^6.1.11",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.1",
    "@types/react-redux-toastr": "^8.0.0",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.56.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss-lit": "^1.1.1",
    "snazzy": "^9.0.0",
    "stylelint": "^16.5.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "typescript": "^5.4.5"
  }
}
